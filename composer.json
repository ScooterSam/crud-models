{
  "name": "scootersam/crud-models",
  "description": "Automatically generate crud api endpoints and vue components for specific models",
  "license": "MIT",
  "keywords": [
    "crud",
    "models"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Sam Parton",
      "email": "sam@idevelopthings.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/dbal": "^2.8",
    "illuminate/support": "~5.6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "ScooterSam\\CrudModels\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScooterSam\\CrudModels\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ScooterSam\\CrudModels\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
